{
  "GeneralInfo": {
    "Name": "Automox",
    "Description": "Automox Rest API",
    "AuthorizationType": "BearerToken",
    "version": "1",
    "Docs": "https://docs.automox.com/api/"
  },
  "Services": {
    "Groups": [
      {
        "Operation": "Groups - List All",
        "Method": "GET",
        "Uri": "https://console.automox.com/api/servergroups/",
        "Description": "Retrieves all server group objects for the authenticated user.",
        "Docs": "https://docs.automox.com/api/groups/list-details-for-all-groups"
      }
    ],
    "Servers": [
      {
        "Operation": "Servers - List All",
        "Method": "GET",
        "Uri": "https://console.automox.com/api/servers/",
        "Description": "Retrieve details for all devices",
        "Docs": "https://docs.automox.com/api/endpoints/get-servers"
      },
      {
        "Operation": "Server - Get",
        "Method": "GET",
        "Uri": "https://console.automox.com/api/servers/{ServerId}",
        "Description": "Returns a specific device (server object) for the authenticated user.",
        "Docs": "https://docs.automox.com/api/endpoints/return-device-details"
      },
      {
        "Operation": "Server - Modify",
        "Method": "PUT",
        "Uri": "https://console.automox.com/api/servers/{ServerId}?server_group_id={ServerGroupId}",
        "Description": "https://docs.automox.com/api/endpoints/modify-a-device",
        "Docs": "Update a device (server object)."
      }
    ]
  }
}